<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="#B7EA5A"/>
            <size
                android:width="14dp"
                android:height="14dp"/>
            <stroke android:width="1.5dp" android:color="#fff"/>
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="#B7EA5A"/>
            <size
                android:width="7dp"
                android:height="7dp"/>
        </shape>
    </item>
</layer-list>

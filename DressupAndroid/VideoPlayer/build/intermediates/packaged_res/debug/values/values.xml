<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="alpha_25_black">#40000000</color>
    <color name="dialog_bg">#bc000000</color>
    <color name="redTab">#D03E49</color>
    <string name="app_name">YCVideoPlayerLib</string>
    <string name="continue_play">继续播放</string>
    <string name="error_message">出了点小故障</string>
    <string name="lock_tip">请先解锁屏幕！</string>
    <string name="locked">已锁定</string>
    <string name="net_wifi_tip">您正在使用移动网络，继续播放将消耗流量</string>
    <string name="replay">重新播放</string>
    <string name="retry">点击重试</string>
    <string name="share">分享视频</string>
    <string name="unlocked">已解锁</string>
    <style name="FloatWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/anim_window_enter</item>
        <item name="android:windowExitAnimation">@anim/anim_window_exit</item>
    </style>
    <style name="dialog_change_clarity" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowBackground">@color/dialog_bg</item>
    </style>
    <style name="speedPopAnim">
            <item name="android:windowEnterAnimation">@anim/slide_in_right</item>
            <item name="android:windowExitAnimation">@anim/slide_out_right</item>
        </style>
    <declare-styleable name="VideoPlayer">
        <attr format="boolean" name="looping"/>
        <attr format="boolean" name="enableAudioFocus"/>
        <attr format="dimension" name="screenScaleType">
            <enum name="type_default" value="0"/>
            <enum name="type_16_9" value="1"/>
            <enum name="type_4_3" value="2"/>
            <enum name="type_match_parent" value="3"/>
            <enum name="type_original" value="4"/>
            <enum name="type_center_crop" value="5"/>
        </attr>
        <attr format="color" name="playerBackgroundColor"/>
    </declare-styleable>
</resources>